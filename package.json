{
  "name": "fulldevgear-portfolio",
  "version": "1.0.0",
  "description": "Portfolio website for FullDevGear - Code, No-Code, AI solutions",
  "scripts": {
    "dev": "http-server -p 3000 -o index.html",
    "start": "http-server -p 8080",
    "lint": "eslint . --ext .js",
    "format": "prettier --write .",
    "build": "node scripts/build.js all",
    "icons": "node scripts/generate-icons.js",
    "optimize:images": "node scripts/optimize-images-run.js --optimize",
    "ci": "npm run lint && npm run build",
    "build:css": "node scripts/build.js css",
    "build:js": "node scripts/build.js js",
    "build:html": "node scripts/build.js html"
  },
  "keywords": [
    "portfolio",
    "full-stack",
    "developer",
    "javascript",
    "web-development"
  ],
  "author": "Issa Azegouar",
  "license": "MIT",
  "devDependencies": {
    "clean-css": "^5.3.3",
    "eslint": "^8.57.0",
    "html-minifier-terser": "^7.2.0",
    "http-server": "^14.1.1",
    "prettier": "^3.3.3",
    "sharp": "^0.33.4",
    "terser": "^5.31.0",
    "workbox-build": "^7.1.1"
  }
}